                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        CHITIETHOADONBANHANG.MAMATHANG, CHITIETHOADONBANHANG.DONGIABAN, CHITIETHOADONBANHANG.SOLUONGBAN, MATHANG.TENMATHANG, MATHANG.DONVITINH, KHACHHANG.TENKHACHHANG, 
