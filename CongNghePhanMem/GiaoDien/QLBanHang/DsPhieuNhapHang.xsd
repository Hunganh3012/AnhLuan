                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        CHITIETPHIEUMUAHANG.MAMATHANG, CHITIETPHIEUMUAHANG.DONGIANHAP, CHITIETPHIEUNHAPHANG.SOLUONGNHAP, MATHANG.TENMATHANG, MATHANG.DONVITINH, NHACUNGCAP.TENNHACUNGCAP, 
                         NHACUNGCAP.SODIENTHOAI, NHACUNGCAP.DIACHI, NHACUNGCAP.EMAIL, NHANVIEN.TENNHANVIEN, PHIEUNHAPHANG.LANNHAP, PHIEUNHAPHANG.NGAYNHAPHANG, PHIEUNHAPHANG.TONGTIEN, 
                         PHIEUNHAPHANG.THANHTOAN, PHIEUNHAPHANG.GHICHU, CHITIETPHIEUNHAPHANG.MAPHIEUNHAPHANG
FROM            CHITIETPHIEUMUAHANG INNER JOIN
                         CHITIETPHIEUNHAPHANG ON CHITIETPHIEUMUAHANG.MACHITIETPMH = CHITIETPHIEUNHAPHANG.MACHITIETPMH INNER JOIN
                         MATHANG ON CHITIETPHIEUMUAHANG.MAMATHANG = MATHANG.MAMATHANG INNER JOIN
                         NHACUNGCAP ON MATHANG.MANHACUNGCAP = NHACUNGCAP.MANHACUNGCAP INNER JOIN
                         PHIEUNHAPHANG ON CHITIETPHIEUNHAPHANG.MAPHIEUNHAPHANG = PHIEUNHAPHANG.MAPHIEUNHAPHANG INNER JOIN
                         NHANVIEN ON PHIEUNHAPHANG.MANHANVIEN = NHANVIEN.MANHANVIEN
WHERE        (CHITIETPHIEUNHAPHANG.MAPHIEUNHAPHANG IN
                             (SELECT        MAPHIEUNHAPHANG
                               FROM            PHIEUNHAPHANG AS PHIEUNHAPHANG_1
                               WHERE        (MAPHIEUNHAPHANG = @mapn)))</CommandText>
